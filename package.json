{
  "name": "workspace",
  "version": "0.1.0",
  "description": "basic webpack setttings out of box",
  "main": "src/index.js",
  "scripts": {
    "prebuid:dll": "rimraf tmp",
    "build:dll": "webpack --config ./build/webpack.dll.js",
    "prestart": "npm run build:dll",
    "start": "NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js",
    "pretest": "npm run build:dll",
    "test": "NODE_ENV=testing karma start karma.conf.js",
    "prerelease": "rimraf dist",
    "release": "NODE_ENV=production webpack --config ./build/webpack.prod.js"
  },
  "keywords": [
    "webpack",
    "hot-module-reload"
  ],
  "author": "Ching Lee <leeching.fx@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.28.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "svg-sprite-loader": "^0.3.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "whatwg-fetch": "^2.0.3"
  }
}
